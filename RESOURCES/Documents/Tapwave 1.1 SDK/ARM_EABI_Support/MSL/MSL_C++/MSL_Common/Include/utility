_MSL_START_TR1_NAMESPACE

class nat {nat();};

template <class T0 = nat, class T1 = nat, class T2 = nat, class T3 = nat, class T4 = nat,
          class T5 = nat, class T6 = nat, class T7 = nat, class T8 = nat, class T9 = nat>
class tuple;

